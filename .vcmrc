{
  "types": [
    "feat",
    "fix",
    "docs",
    "style",
    "refactor",
    "perf",
    "test",
    "build",
    "ci",
    "revert"
  ],
  "scope": {
    "required": true,
    "allowed": [
      "all",
      "smartwallet",
      "smartwallet-e2e",
      "ui",
      "ui-e2e"
